{
  "name": "elite-bot",
  "version": "1.0.0",
  "description": "",
  "author": "Chris Smola",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^v8.10.0",
    "npm": "^5.6.0"
  },
  "scripts": {
    "postinstall": "npm run docs",
    "start": "node index.js",
    "start:docker": "docker -i -t -p 5000:5000 $IMAGE_TAG",
    "start:watch": "nodemon --config ./elitebot.nodemon.json",
    "docs": "webpack -c webpack.config.js && jsdoc -c ./jsdoc.conf.json",
    "docs:watch": "nodemon --config ./docs.nodemon.json",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint --fix '**/*.js'",
    "test": "NODE_ENV=test jasmine --config=./jasmine.json",
    "test:version": "./bin/version-diff",
    "test:watch": "nodemon --config ./spec.nodemon.json",
    "jasmine": "jasmine",
    "jsdoc": "jsdoc",
    "autotest": "jasmine --color --autotest spec/ --watch models",
    "bootstrap-dev": "NODE_ENV=development ./bin/create-config && ./bin/create-tls-cert",
    "bump": "npm run docs && npm --no-git-tag-version version",
    "bump:minor": "npm run bump -- minor",
    "bump:patch": "npm run bump -- patch",
    "create-tls-cert": "./bin/create-tls-cert",
    "create-config": "./bin/create-config"
  },
  "dependencies": {
    "@octokit/rest": "^15.8.1",
    "@slack/client": "^4.2.2",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "semantic-ui-react": "^0.85.0",
    "toast-jsdoc": "^1.0.2",
    "traverse": "^0.6.6",
    "tui-jsdoc-template": "^1.2.2",
    "winston": "^3.0.0-rc6",
    "winston-daily-rotate-file": "^3.2.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nock": "^9.3.0",
    "nodemon": "^1.17.5",
    "request": "^2.87.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  }
}
