{
  "name": "slapps",
  "version": "1.0.0-5",
  "description": "",
  "author": "Chris Smola",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^v8.10.0",
    "npm": "^5.6.0"
  },
  "scripts": {
    "postinstall": "npm run docs",
    "start:nectarbot": "node nectarbot.js",
    "start:nectarbot:docker": "docker -i -t -p 5000:5000 $IMAGE_TAG",
    "start:nectarbot:watch": "nodemon --config ./nectarbot.nodemon.json",
    "docs": "jsdoc -c ./jsdoc.conf.json",
    "docs:watch": "nodemon --config ./docs.nodemon.json",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint --fix '**/*.js'",
    "test": "NODE_ENV=test jasmine --config=./jasmine.json",
    "test:version": "./bin/version-diff",
    "test:watch": "nodemon --config ./spec.nodemon.json",
    "jasmine": "jasmine",
    "jsdoc": "jsdoc",
    "autotest": "jasmine --color --autotest spec/ --watch models",
    "bootstrap-dev": "NODE_ENV=development ./bin/create-config && ./bin/create-tls-cert && touch .now.env.development",
    "bump": "npm run docs && npm --no-git-tag-version version",
    "bump:minor": "npm run bump -- minor",
    "bump:patch": "npm run bump -- patch",
    "create-tls-cert": "./bin/create-tls-cert",
    "create-config": "./bin/create-config"
  },
  "dependencies": {
    "@octokit/rest": "^15.8.1",
    "@slack/client": "^4.2.2",
    "aws-sdk": "^2.249.1",
    "jenkins": "^0.20.1",
    "jsdoc": "^3.5.5",
    "lodash": "^4.17.11",
    "lokijs": "^1.5.5",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "toast-jsdoc": "^1.0.2",
    "traverse": "^0.6.6",
    "tui-jsdoc-template": "^1.2.2",
    "winston": "^3.0.0-rc6",
    "winston-daily-rotate-file": "^3.2.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nock": "^9.3.0",
    "nodemon": "^1.17.5",
    "request": "^2.87.0"
  }
}
